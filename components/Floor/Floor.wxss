.floor-box .floor-group .floor-title {
  padding-top: 8rpx;
}

.floor-box .floor-group .floor-title image {
  width: 100%;
}

.floor-box .floor-group .floor-list {
  overflow: hidden;
}

.floor-box .floor-group .floor-list navigator {
  float: left;
  width: 33.33%;
}

.floor-box .floor-group .floor-list navigator:nth-last-child(-n+4) {
  height: 27.72711vw;
  padding-left: 10rpx;
}

.floor-box .floor-group .floor-list navigator:nth-child(2), .floor-box .floor-group .floor-list navigator:nth-child(3) {
  padding-bottom: 8rpx;
}

.floor-box .floor-group .floor-list navigator image {
  width: 100%;
  height: 100%;
}
